<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id = "msg" class = " org.example.model.Message">
        <property name = "message" value = "Krishna !"/>
    </bean>
    <bean id="ticket" class="org.example.model.Ticket.Ticket">
        <constructor-arg index = "0" value = "101"/>
        <constructor-arg index = "1" value = "24/9/2019"/>
    </bean>
    <bean id = "student" class = " org.example.model.Setterbased">
        <property name = "stdid" value = "1"/>
        <property name = "name" value = "lala"/>
        <property name = "coursename" value = "engineering"/>
    </bean>


</beans>